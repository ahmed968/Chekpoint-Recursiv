ALGORITHM Ticket Pricing:
VAR
    age:INTEGER
BEGIN
    write ("enter your age: ")
    read(age)
        IF (age <= 12) THEN
            Write("Your ticket price is $10")
        ELSE_IF (age <= 17) THEN
             Write("Your ticket price is $15")
        ELSE
            Write("Your ticket price is $20")
        END_IF
END